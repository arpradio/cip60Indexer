{
  "name": "musicindex",
  "version": "0.1.0-alpha",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
    "start:indexer": "ts-node --project tsconfig.node.json src/indexer/index.ts",
    "start:api": "ts-node --project tsconfig.node.json src/api/server.ts",
    "build:dashboard": "npx webpack --mode production",
    "watch:dashboard": "npx webpack --watch --mode development",
    "dev": "concurrently \"npm run start:indexer\" \"npm run start:api\" \"npm run watch:dashboard\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tanstack/react-table": "^8.20.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "lucide-react": "^0.474.0",
    "pg": "^8.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "cardano",
    "musicNFT",
    "blockchain",
    "indexer",
    "cip60"
  ],
  "author": "sudoScientist",
  "license": "MIT",
  "description": "A blockchain indexer for CIP-60 compliant Music NFTs on the Cardano network."
}
